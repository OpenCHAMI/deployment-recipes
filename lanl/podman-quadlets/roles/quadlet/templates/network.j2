[Unit]
Description={{ item.name }} Network

[Network]
NetworkName={{ item.name }}
{% if item.internal is defined %}
Internal={{ item.internal }}
{% else %}
Internal=false
{% endif %}
{% if item.subnet is defined %}
Subnet={{ item.subnet }}
{% endif %}
{% if item.driver is defined %}
Driver={{ item.driver }}
Options={{ item.opts }}
{% endif %}
