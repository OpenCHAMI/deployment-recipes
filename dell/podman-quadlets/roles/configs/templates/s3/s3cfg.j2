# Setup endpoint
host_base = {{ cluster_name }}.{{ cluster_domain }}:9000
host_bucket = {{ cluster_name }}.{{ cluster_domain }}:9000
bucket_location = us-east-1
use_https = False

# Setup access keys
access_key = {{ minio_s3_username }}
secret_key = {{ minio_s3_password }}

# Enable S3 v4 signature APIs
signature_v2 = False